{
  "name": "@streaming-hell/telegram-bot",
  "version": "0.2.2",
  "description": "Telegram bot for share music with anyone no matter the platform",
  "keywords": [
    "telegram",
    "telegram bot",
    "telegraf"
  ],
  "homepage": "https://github.com/streaming-hell/telegram-bot#readme",
  "bugs": {
    "url": "https://github.com/streaming-hell/telegram-bot/issues",
    "email": "aleksandr@bukhalo.com"
  },
  "license": "GPL-3.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn build && NODE_ENV=prod node dist/main.js",
    "build": "rm -rf ./dist && NODE_ENV=prod babel ./src -d dist --extensions '.ts'",
    "dev": "NODE_ENV=dev nodemon --exec babel-node --extensions '.ts' --inspect src/main.ts",
    "deploy": "env-cmd serverless deploy"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.11",
    "telegraf": "^3.27.1",
    "telegraf-mixpanel": "^1.0.0",
    "validator": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/aws-lambda": "^8.10.31",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "env-cmd": "^10.0.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.19.2",
    "prettier": "^1.16.4",
    "serverless": "^1.52.2",
    "typescript": "^3.6.3"
  }
}
